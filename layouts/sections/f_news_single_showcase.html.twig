{% if content %}
	<article {{attributes.addClass('single-showcase_fnews')}}>
		<div class="container">
			<div class="row flex-wrap-reverse">
				<div class="col-md-8">
					{% if content.full_image %}
						<a href="#" {{ region_attributes.full_image.addClass('ssf__img ')}}>
							{{ content.full_image }}
						</a>
					{% endif %}
					<div class="my-5">
						{% if content.paragraph_one %}
							<p {{ region_attributes.paragraph_one.addClass('ssf-paragraph')}}>
								{{ content.paragraph_one }}
							</p>
						{% endif %}
					</div>
                    <div class="video-handler">
					{% if content.full_image_video %}
						<div {{region_attributes.full_image_video.addClass('video-sup')}}>
							{{ content.full_image_video }}
						</div>
					{% endif %}
                    </div>
                    {% if content.tag_list_item %}
					<div class="tag-list">
                    {#{region_attributes.tag_list_item.addClass('fn-tag-btn ')}#}
					    {{ content.tag_list_item }}
					</div>
                    {% endif %}
				</div>
				<div class="col-md-4">
                    {% if content %}
                    <div {{ attributes.addClass('sticky-author-fnews')}}>
                        <div class="saf-author">
                            {% if content.sticky_author_image %}
                            <div {{ region_attributes.sticky_author_image.addClass('saf-author__img')}}>
                                Â {{ content.sticky_author_image }}
                            </div>
                            {% endif %}
                            {% if content.sticky_author_role %}
                            <div {{ region_attributes.sticky_author_role.addClass('saf-author__title') }}>
                                {{ content.sticky_author_role }}
                            </div>
                            {% endif %}
                        </div>
                        <div class="saf-desc">
                            {% if content.sticky_author_pub %}
                                <div {{ region_attributes.sticky_author_pub.addClass('saf-desc__title') }}>
                                    {{ content.sticky_author_pub }}
                                </div>
                            {% endif %}
                            {% if content.sticky_author_date_one %}
                                <div {{ region_attributes.sticky_author_date_one.addClass('saf-desc__desc')}}>
                                    {{ content.sticky_author_date_one }}
                                </div>
                            {% endif %}
                        </div>
                        <div class="saf-desc">
                            {% if content.sticky_author_update %}
                                <div {{ region_attributes.sticky_author_update.addClass('saf-desc__title')}}>
                                    {{ content.sticky_author_update }}
                                </div>
                            {% endif %}
                            {% if content.sticky_author_date_two %}
                                <div {{ region_attributes.sticky_author_date_two.addClass('saf-desc__desc')}}>
                                    {{ content.sticky_author_date_two }}
                                </div>
                            {% endif %}
                        </div>
                        <div class="saf-desc">
                            {% if content.sticky_author_number %}
                                <div {{ region_attributes.sticky_author_number.addClass('saf-desc__title')}}>
                                    {{ content.sticky_author_number }}
                                </div>
                            {% endif %}
                            {% if content.sticky_author_read %}
                                <div {{ region_attributes.sticky_author_read.addClass('saf-desc__desc')}}>
                                    {{ content.sticky_author_read }}
                                </div>
                            {% endif %}
                        </div>
                        <div class="saf-btn">
                            {% if content.sticky_author_status %}
                                <div {{ region_attributes.sticky_author_status.addClass('saf-btn__btn')}}>
                                    {{ content.sticky_author_status }}
                                </div>
                            {% endif %}
                            {% if content.sticky_author_status_one %}
                                <div {{ region_attributes.sticky_author_status_one.addClass('saf-btn__btn')}}>
                                    {{ content.sticky_author_status_one }}
                                </div>
                            {% endif %}
                        </div>
                        {% if content.sticky_author_icon_contener %}
                        <div {{ region_attributes.sticky_author_icon_contener.addClass('saf-social')}}>
                            {{ content.sticky_author_icon_contener }}
                        </div>
                        {% endif %}
                    </div>
                    {% endif %}

                </div>
			</div>
		</div>
	</article>
{% endif %}
